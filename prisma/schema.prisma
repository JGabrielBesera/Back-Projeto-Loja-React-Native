generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model produtos {
  id        Int    @id @default(autoincrement())
  nome      String @db.VarChar(33)
  photo     String @db.VarChar(255)
  preco     Int
  descricao String @db.VarChar(255)
  categoria Int
}
